name: Debug CI

on:
  push:
    #paths: 
    #  - '001/**'
    branches: 
      - main
      - develop

jobs:
  debug:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout
        uses: actions/checkout@v4
      
      - name: Debug Info
        run: |
          echo "=== DEBUG WORKFLOW EJECUT√ÅNDOSE ==="
          echo "Rama actual: ${{ github.ref }}"
          echo "Commit: ${{ github.sha }}"
          echo "Archivos en 001/:"
          ls -la 001/ || echo "Directorio 001/ no encontrado"
          echo "Directorio de trabajo:"
          pwd
          echo "Archivos modificados:"
          git diff --name-only HEAD~1 HEAD || echo "No se puede obtener diff"